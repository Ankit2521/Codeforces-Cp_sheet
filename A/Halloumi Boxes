#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t; // Input the number of test cases
    while (t--) {
        int n, k;
        cin >> n >> k;
        vector<int> arr(n);
        for (int i = 0; i < n; i++) {
            cin >> arr[i];
        }

        if (k == 1 && !is_sorted(arr.begin(), arr.end())) {
            cout << "No" << '\n';
        } else {
            cout << "Yes" << '\n';
        }
    }
    return 0;
}
